{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": 0,
        "no-restricted-globals": 0,
        "consistent-return": 0,
        "object-shortland": 0
    }
}
